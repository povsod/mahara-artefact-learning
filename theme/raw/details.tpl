{include file="header.tpl"}{if $tags}<div>{str tag=tags}: {list_tags tags=$tags owner=$owner}</div>{/if}<p>{$description|clean_html|safe}<p>{if $learning->files.stage0}<div id="learningfiles_{$id}">    <table class="cb attachments fullwidth">        <thead class="expandable-head">            <tr>                <td colspan="2">                    <a class="toggle" href="#">{str tag=attachedfiles section=artefact.blog}</a>                    <span class="fr">                        <img class="fr" src="{theme_url filename='images/attachment.png'}" alt="{str tag=Attachments section=artefact.resume}">                        {count($learning->files.stage0)}                    </span>                </td>            </tr>        </thead>        <tbody class="expandable-body">            {foreach from=$learning->files.stage0 item=file}            <tr class="{cycle values='r1,r0'}">                <td class="icon-container"><img src="{$file->icon}" alt=""></td>                <td><h3 class="title"><a href="{$WWWROOT}artefact/file/download.php?file={$file->id}">{$file->title}</a></h3>                <div class="detail">{$file->description}</div></td>            </tr>            {/foreach}        </tbody>    </table></div>{/if}{if $learning->priorknowledge}<h3>{str tag=stage1title section=artefact.learning}</h3><p>{$learning->priorknowledge|clean_html|safe}</p>{if $learning->files.stage1}<div id="learningfiles_stage1_{$id}">    <table class="cb attachments fullwidth">        <thead class="expandable-head">            <tr>                <td colspan="2">                    <a class="toggle" href="#">{str tag=attachedfiles section=artefact.blog}</a>                    <span class="fr">                        <img class="fr" src="{theme_url filename='images/attachment.png'}" alt="{str tag=Attachments section=artefact.resume}">                        {count($learning->files.stage1)}                    </span>                </td>    